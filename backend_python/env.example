# Database (обязательно)
DATABASE_URL=postgresql://user:password@host:port/database

# JWT Secret (обязательно для production)
JWT_SECRET=your-secret-key-change-in-production-min-32-chars

# CORS (опционально)
CORS_ORIGINS=https://web.telegram.org,https://telegram.org

# Frontend URL (опционально)
FRONTEND_URL=https://your-frontend-url.com

# File upload settings (опционально)
MAX_FILE_SIZE=5242880

# ImageKit settings (обязательно для загрузки фотографий)
# Формат:
# IMAGEKIT_PUBLIC_KEY=public_xxxxxxxxxxxxx
# IMAGEKIT_PRIVATE_KEY=private_xxxxxxxxxxxxx
# IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_imagekit_id
# Пример:
# IMAGEKIT_PUBLIC_KEY=public_/ZvVSOGo+d6AjTYXwaol4j2Nbeo=
# IMAGEKIT_PRIVATE_KEY=private_7EBDrt4MHu7adp27PgGHfM9kz2Y=
# IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/ml7fzwzn0
IMAGEKIT_PUBLIC_KEY=your_public_key_here
IMAGEKIT_PRIVATE_KEY=your_private_key_here
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_imagekit_id

# Telegram Bot Token (опционально, если используете)
TELEGRAM_BOT_TOKEN=your_bot_token


